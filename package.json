{
  "name": "localink",
  "version": "1.0.0",
  "description": "Web application for localink ",
  "main": "index.js",
  "dependencies": {
    "axios": "^0.19.2",
    "bcryptjs": "^2.4.3",
    "client": "file:client",
    "config": "^3.3.0",
    "express": "^4.17.1",
    "express-validator": "^6.4.0",
    "gravatar": "^1.8.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.5",
    "normalize-url": "^5.0.0"
  },
  "devDependencies": {
    "concurrently": "^5.1.0",
    "nodemon": "^2.0.2"
  },
  "scripts": {
    "server-install": "cd backend && npm install",
    "client-install": "cd frontend && npm install",
    "both-install": "npm run server-install &&  npm run client-install",
    "server-run": "cd backend && npm run server",
    "client-run": "cd frontend && npm start",
    "dev": "concurrently \"npm run server-run\" \"npm run client-run\""
  },
