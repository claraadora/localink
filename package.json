    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-validator": "^6.5.0",
    "google-auth-library": "^6.0.2",
    "got": "^11.3.0",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^3.5.7",
    "mongoose": "^5.9.20",
    "nodemailer": "^6.4.8",
    "socket.io": "^2.3.0"
  }
